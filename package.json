{
  "name": "nautilis",
  "version": "1.0.0",
  "description": "A Docker Compose Charting Tool",
  "types": "./src/types.d.ts",
  "scripts": {
    "electron": "electron .",
    "start": "cross-env NODE_ENV=production node dist/server.js",
    "build": "npm run build-src && npm run build-server",
    "build-src": "cross-env NODE_ENV=production webpack",
    "build-server": "tsc",
    "watch-ts": "tsc -w",
    "watch-node": "cross-env NODE_ENV=development nodemon dist/server.js",
    "watch": "concurrently \"npm run watch-ts\" \"npm run watch-node\"",
    "dev": "concurrently \"cross-env NODE_ENV=development webpack-dev-server --open\" \"npm run watch\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oslabs-beta/nautilis.git"
  },
  "keywords": [
    "docker",
    "compose",
    "visualizer"
  ],
  "author": "team nautilis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oslabs-beta/nautilis/issues"
  },
  "homepage": "https://github.com/oslabs-beta/nautilis#readme",
  "dependencies": {
    "@types/js-yaml": "^3.12.2",
    "bootstrap": "^4.4.1",
    "eslint": "^6.8.0",
    "express": "^4.17.1",
    "jquery": "^3.4.1",
    "js-yaml": "^3.13.1",
    "multer": "^1.4.2",
    "node-sass": "^4.13.1",
    "react": "^16.13.0",
    "react-bootstrap": "^1.0.0-beta.17",
    "react-dom": "^16.13.0"
  },
  "devDependencies": {
    "@types/eslint": "^6.1.8",
    "@types/express": "^4.17.3",
    "@types/node": "^13.9.0",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.4.2",
    "electron": "^8.1.1",
    "mini-css-extract-plugin": "^0.9.0",
    "nodemon": "^2.0.2",
    "react-hot-loader": "^4.12.19",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.1.3",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
